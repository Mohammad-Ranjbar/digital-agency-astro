---
import '@fortawesome/fontawesome-free/css/all.min.css'
import '../libraries/animate/animate.min.css'
import '../libraries/owlcarousel/assets/owl.carousel.min.css'
import '../libraries/lightbox/css/lightbox.min.css'
import '../styles/bootstrap.min.css'
import '../styles/style.css'
import Footer from '../components/Footer.astro'
export interface Props {
    title?: string;
}
const {title = 'Digital Agency'} = Astro.props as Props;
---
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>{title}</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content={title} name="keywords">
    <meta content={title} name="description">

</head>
<body>
<div class="container-xxl bg-white p-0">
    <slot/>
    <Footer/>
</div>

<script>
    import "../scripts/jQuery.js"
    import 'bootstrap/dist/js/bootstrap.bundle.min'
    import "../libraries/easing/easing.min.js"
    import "../libraries/waypoints/waypoints.min.js"
    import "../libraries/counterup/counterup.min.js"
    import "../libraries/owlcarousel/owl.carousel.min.js"
    import "../libraries/isotope/isotope.pkgd.min.js"
    import "../scripts/main.js"
</script>
</body>
</html>